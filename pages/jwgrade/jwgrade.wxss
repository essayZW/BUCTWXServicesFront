/* pages/jw/jw.wxss */
page{
  height: 100%;
}
.main{
  width:100%;
  height:100%;
  position: absolute;
  top:0px;
  left:0px;
  z-index: 2;
  background-color: white;
  transition: left 0.7s;
}
/* 调整选择列表的显示区域宽度 */
.selected-list{
  left:60%;
}

.select-list{
  height:100%;
  width:100%;
  position: fixed;
  top:0px;
  left:0px;
}
.subject{
  /*border-top: 10rpx solid #dedede;*/
  border-bottom: 10rpx solid #f5f5f5;
  border-top: 10rpx solid #f5f5f5;
  border-left: 23rpx solid #f5f5f5;
  border-right: 23rpx solid #f5f5f5;
  background-color: white;
}
.logo-s{
  display: inline-block;
}
.logo-text{
  display: inline-block;
  margin-left: 30rpx;
  vertical-align: top;
}
.logo-texta{
  float:right;
  margin-right: 20rpx;
}
.logo-text-style{
  font-size: 15px;
  line-height:10px;
  color: #636363;
}
.subject-name{
  margin-top:15rpx;
  margin-bottom:20rpx;
}
.bkg{
  height:140rpx;
  background:#fad258;
  overflow: hidden;
  display: flex;
  align-items: center;
}/*ffd21e*//*#fad258*/
.line{
  margin-left:120rpx;
  width:100%;
  height:1px;
  background:#f3f2f2;
}

.item{
  margin-left:15rpx;
  width:20rpx;
  height:20rpx;
}
.logo{
  margin-left:30rpx;
  margin-bottom:2rpx;
  margin-top:12rpx;
  width:70rpx;
  height:70rpx;
}
.detailed-grade{
  float:right;
  margin-right: 20rpx;
}
.detailed-grade-text{
  font-size: 10px;
  color:#636363;
}
.details{
  width:13rpx;
  height:13rpx;
}
.text-middle-shadow{
  background: linear-gradient(to bottom, white 60%, #f7b52c 40%);
  font-size: 18px;
  color:#636363;
  
}
.index-normal{
  font-size: 18px;
  color:#636363;
}

.item-line{
  height:100rpx;
  line-height: 100rpx;
}

.no-grade{
  text-align: center;
  color: gray;
  margin-top: 30rpx;
}

.menu-img{
  width:35rpx;
  height:40%;
  margin-left: -10rpx;
}

.head-img{
  width:105rpx;
  height:105rpx;
  margin-left:10rpx;
  border:4rpx solid white;
}

.student-info-show{
  display: inline-block;
  height:105rpx;
  min-width:60%;
  margin-left: 10rpx;
}
.student-info-show>view{
  height: 50%;
  width:100%;
  color:white;
}

.select-area{
  width:60%;
  height:300rpx;
  margin-left: 50rpx;
}
.selected-info-show{
  margin-bottom:20rpx;
  margin-left:10rpx;
}
.query-button{
  float:left;
}