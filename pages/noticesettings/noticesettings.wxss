/* pages/noticesettings/noticesettings.wxss */
page{
    background-color: rgb(246, 246, 246);
}
.settings-item{
    width:100%;
    height:120rpx;
    margin-top:20rpx;
}

.settings-item>view{
    height:50%;
    line-height: 60rpx;
    padding-left: 20rpx;
}
.settings-item>picker{
    height:50%;
    background-color: white;
    line-height: 60rpx;
    padding-left: 20rpx;
}
.confirm-butt{
    width:110rpx;
    height:110rpx;
    border-radius: 50%;
    margin:50rpx auto;
}
.confirm-butt>image{
    width:100%;
    height:100%;
}