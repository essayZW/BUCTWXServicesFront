.flex {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.root {
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
}

.boxBorderContainer {
  box-sizing: border-box;
}

.color-picker {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
}

.main-container {
  width: 100%;
  height: 50%;
}

.color-ring-panel {
  border: solid 2px lightgray;
}

.jianbian-a {
  background-image: linear-gradient(45deg, #00ffff 5%, #009dff, #003fff, #2200ff, #7f00ff 95%);
  border-radius: 100% 0 0 0;
  width: 100%;
  height: 100%;
}

.jianbian-b {
  background-image: linear-gradient(-45deg, #ff0000 5%, #ff008c, #ff00bf, #dc00ff, #7f00ff 95%);
  border-radius: 0 100% 0 0;
  width: 100%;
  height: 100%;
}

.jianbian-c {
  background-image: linear-gradient(-45deg, #7fff00 5%, #21ff00, #00ff3f, #00ff9d, #00ffff 95%);
  border-radius: 0 0 0 100%;
  width: 100%;
  height: 100%;
}

.jianbian-d {
  background-image: linear-gradient(45deg, #7fff00 5%, #ddff00, #ffbf00, #ff6100, #ff0000 95%);
  border-radius: 0 0 100% 0;
  width: 100%;
  height: 100%;
}

.ring-pointer {
  border-radius: 50%;
  background-color: whitesmoke;
  box-shadow: 0px 0px 6px grey;
}

.center-view {
  box-shadow: 2px 2px 5px grey;
}
.center-view-bcg{
  box-shadow: 2px 2px 5px grey;
  background-image: url("data:image/jpg;base64,")
}

.centerBg {
  background-color: white
}

.grid {
  display: grid;
  grid-template-columns: 50% 50%;
}

.flex-column {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.slider-pointer {
  background-color: whitesmoke;
  border-radius: 50%;
  box-shadow: 0px 0px 5px grey;
}


.sliderStripe {
  border-radius: 5px;
  border: solid 1px lightgray;
}

.opacity-background{
  background-image: url("data:image/jpg;base64,");
  background-position: right;
}

.colorCard {
  border-radius: 5px;
  box-shadow: 2px 0px 2px grey;
  display: inline-block;
}